	},
	"homepage": "https://github.com/infinum/auth-worker#readme",
	"devDependencies": {
		"@infinumjs/eslint-config-core-ts": "^3.3.1",
		"@typescript-eslint/parser": "^5.59.2",
		"eslint": "^8.39.0",
		"lint-staged": "^13.2.2",
		"nodemon": "^2.0.22",
		"prettier": "^2.8.8",
		"tsup": "^6.7.0",
		"typescript": "^5.0.4"
	},
